{
  "name": "philip",
  "version": "2.0.2",
  "description": "Process files and directories in DOM File System",
  "main": "legacy/philip.js",
  "dependencies": {
    "domfs-findit": "^2.0.0",
    "emitify": "^4.0.1",
    "inherits": "^2.0.3",
